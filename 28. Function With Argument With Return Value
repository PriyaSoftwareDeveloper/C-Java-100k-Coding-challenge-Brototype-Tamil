#include <stdio.h>
#include <stdlib.h>

int sum(int,int);
int main(void) {
	int a,b;
	int res;
	setbuf(stdout, NULL);
	printf("Enter two numbers: ");
	scanf("%d%d", &a, &b);
	res=sum(a,b);
	printf("The Result is: %d", res);
	return EXIT_SUCCESS;
}

int sum(int num1, int num2){
	int result;

	result = num1 + num2;
	return result;
}
