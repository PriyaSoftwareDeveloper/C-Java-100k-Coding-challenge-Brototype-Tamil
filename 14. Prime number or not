
#include <stdio.h>
#include <stdlib.h>

int main(void) {
	int n=0, num, i, flag;
	setbuf(stdout, NULL);
	printf("Enter a number: ");
	scanf("%d", &num);
	for(i=2; i<n/2; i++){
		if(n%i==0){
			flag = 1;
			break;
		}
	} if(flag==0){
		printf("Prime Number");
	} else {
		printf("Not a Prime Number");
	}
	return EXIT_SUCCESS;
}
